<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="stocks" type="stocksType"/>
  <xs:complexType name="stockType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:string" name="symbol" use="optional"/>
        <xs:attribute type="xs:string" name="price" use="optional"/>
        <xs:attribute type="xs:string" name="time" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="stocksType">
    <xs:sequence>
      <xs:element type="stockType" name="stock" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>